<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.meilituibian.api.mapper.ProductOrderMapper">
    <insert id="saveProductOrder" parameterType="ProductOrder" keyProperty="id" useGeneratedKeys="true">
        insert into product_order(order_no,user_id,open_id,product_id,product_name,quantity,address,phone,remark)
        values(#{orderNo},#{userId},#{openId},#{productId},#{productName},#{quantity},#{address},#{phone},#{remark})
    </insert>


</mapper>


